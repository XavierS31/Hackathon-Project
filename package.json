{
  "name": "knighthaven",
  "version": "1.0.0",
  "description": "KnightHaven - A local social and marketplace web app for the UCF community",
  "type": "module",
  "main": "app.js",
  "scripts": {
    "dev": "node app.js",
    "api": "node api-server.js",
    "frontend": "cd frontend && python3 -m http.server 8080",
    "start": "npm run api",
    "start:all": "./start-all.sh",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "node dbconnect.js"
  },
  "devDependencies": {
    "prisma": "^6.18.0"
  },
  "dependencies": {
    "@prisma/client": "^6.18.0",
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1"
  }
}
